{
  "name": "checkin",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:webbower/checkin.git",
  "author": "Matt Bower (http://webbower.com)",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write",
    "format:check": "prettier-check .",
    "lint": "eslint .",
    "lint:dev": "eslint --fix . && echo 'Lint pass.' || echo 'Lint failure'",
    "preflight": "yarn run lint && yarn run format:check",
    "test": "NODE_ENV=test riteway -r @babel/register -r regenerator-runtime -r ignore-styles src/**/*.test.js src/**/*.test.jsx",
    "test:dev": "NODE_ENV=test riteway -r @babel/register -r regenerator-runtime -r ignore-styles src/tests.dev.js | tap-color && echo 'Test pass.'",
    "test:watch": "chokidar src/**/*.test.js src/**/*.test.jsx -c \"clear && yarn run -s test:dev\" --initial --silent",
    "watch": "chokidar src -c \"clear && yarn run -s lint:dev && yarn run -s test:dev\" --initial --silent"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/register": "7.10.1",
    "chokidar": "3.4.0",
    "chokidar-cli": "2.1.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "ignore-styles": "5.0.1",
    "prettier": "2.0.5",
    "prettier-check": "2.0.0",
    "regenerator-runtime": "0.13.5",
    "riteway": "6.1.2",
    "sass": "1.26.9",
    "tap-color": "1.2.0"
  },
  "dependencies": {
    "date-fns": "2.14.0",
    "immer": "6.0.9",
    "next": "9.4.4",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8"
  }
}
